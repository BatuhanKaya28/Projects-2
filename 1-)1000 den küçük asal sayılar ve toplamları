#include <stdio.h>
#include <stdlib.h>


int main()
{
    int i,j,sayac=0;
    int sayi=0;
    for(i=2;i<1000;i++)
    {
        for(j=1;j<=i;j++)
        {
            if(i%j==0)
                sayac++;
        }
            if(sayac==2)
        {
            sayac=i;
            sayi = sayi+1;
            printf("%d bir asal sayidir\n",sayac);
        }
        sayac=0;
    }

    printf("\n\nToplamda %d tane asal sayi vardir 1 den 1000e kadar",sayi);

}
